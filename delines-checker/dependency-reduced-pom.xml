<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>delines-parent</artifactId>
    <groupId>com.tangyujun</groupId>
    <version>1.3.0</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>delines-checker</artifactId>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.10.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
          <annotationProcessors>
            <annotationProcessor>com.tangyujun.delines.processor.DelinesEntityPatternProcessor</annotationProcessor>
            <annotationProcessor>com.tangyujun.delines.processor.DelinesFieldPatternProcessor</annotationProcessor>
            <annotationProcessor>com.tangyujun.delines.processor.DelinesEntityCreatorProcessor</annotationProcessor>
            <annotationProcessor>com.tangyujun.delines.processor.DelinesNestedFieldProcessor</annotationProcessor>
            <annotationProcessor>com.tangyujun.delines.validator.processor.BooleanValidationProcessor</annotationProcessor>
            <annotationProcessor>com.tangyujun.delines.validator.processor.DateValidationProcessor</annotationProcessor>
            <annotationProcessor>com.tangyujun.delines.validator.processor.DecimalValidationProcessor</annotationProcessor>
            <annotationProcessor>com.tangyujun.delines.validator.processor.ObjectValidationProcessor</annotationProcessor>
            <annotationProcessor>com.tangyujun.delines.validator.processor.StringValidationProcessor</annotationProcessor>
          </annotationProcessors>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <properties>
    <maven.compiler.target>8</maven.compiler.target>
    <maven.compiler.source>8</maven.compiler.source>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>
